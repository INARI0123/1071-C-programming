#include <stdio.h>
int main()
{
	int h1,h2;
	float x,y;
	
	while(1)
	{
	printf("Enter height(-1 to exit): ");
	scanf("%d %d ",&h1,&h2);
	if(h1==-1 || h2==-1)
		{
		printf("Bye! Coding by your_ID\n");
		break;
		}
	  for(int i=h1;i<=h2;i++)
	  {
	  	x=18.5*(i*i)/10000.;
		y=24*(i*i)/10000. ;
		printf("%d cm: %.1f ~ %.1f (kg)\n",i,x,y);
	  }
	
	}



}
