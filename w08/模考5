void prchar(char c,int n)
{
    for(int i=1;i<=n;i++)
    {
        printf("%c",c);
    }
}

void drawRect(int height,int width)
{


        for(int i=1;i<=width;i++)
        {
            for(int j=1;j<=height;j++)
            {
                printf("*");
            }
                printf("*");

                printf("\n");

        }
}


int main()
{
    int height,width;
    printf("Enter height and width: ");
    scanf("%d %d",&height,&width);

    drawRect(height,width);
}


